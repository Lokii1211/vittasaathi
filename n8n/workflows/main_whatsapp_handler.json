            ],
            "webhookId": "whatsapp-webhook"
        },
        {
            "parameters": {
                "jsCode": "// Extract Twilio WhatsApp data\nconst data = $input.item.json.body || $input.item.json;\n\nlet phone = (data.From || '').replace('whatsapp:', '').trim();\nlet message = (data.Body || 'hi').trim();\nlet mediaUrl = data.MediaUrl0 || null;\nlet mediaType = data.MediaContentType0 || null;\nlet profileName = data.ProfileName || 'Friend';\n\n// Ensure phone starts with +\nif (phone && !phone.startsWith('+')) {\n  phone = '+' + phone;\n}\n\n// Clean phone number\nphone = phone.replace(/[^+0-9]/g, '');\n\nconsole.log('ðŸ“± Received from:', phone, 'Message:', message);\n\nreturn {\n  phone: phone,\n  message: message || 'hi',\n  message_type: mediaType && mediaType.includes('audio') ? 'voice' : 'text',\n  voice_url: mediaUrl,\n  profileName: profileName\n};"
            },
            "id": "extract-1",
            "name": "Extract Message Data",
            "type": "n8n-nodes-base.code",
            "typeVersion": 2,
            "position": [
                420,
                300
            ]
        },
        {
            "parameters": {
                "method": "POST",
                "url": "https://vittasaathi-1.onrender.com/webhook",
                "sendBody": true,
                "specifyBody": "json",
                "jsonBody": "={{ JSON.stringify({ phone: $json.phone, message: $json.message, message_type: $json.message_type, voice_url: $json.voice_url }) }}",
                "options": {
                    "timeout": 45000
                }
            },
            "id": "call-api-1",
            "name": "Call VittaSaathi API",
            "type": "n8n-nodes-base.httpRequest",
            "typeVersion": 4.1,
            "position": [
                640,
                300
            ],
            "onError": "continueRegularOutput"
        },
        {
            "parameters": {
                "jsCode": "const apiResponse = $input.item.json;\nconst userData = $('Extract Message Data').item.json;\n\nconsole.log('ðŸ“¨ API Response:', JSON.stringify(apiResponse));\n\n// Get reply text from API response\nlet replyText = '';\n\nif (apiResponse.reply_text) {\n  replyText = apiResponse.reply_text;\n} else if (apiResponse.message) {\n  replyText = apiResponse.message;\n} else if (apiResponse.text) {\n  replyText = apiResponse.text;\n} else if (typeof apiResponse === 'string') {\n  replyText = apiResponse;\n} else {\n  replyText = 'âœ… Message received!';\n}\n\n// Clean phone number\nlet cleanPhone = userData.phone.replace(/[^+0-9]/g, '');\n\nconsole.log('ðŸ“¤ Reply to:', cleanPhone, 'Message:', replyText.substring(0, 50));\n\nreturn {\n  phone: cleanPhone,\n  message: replyText,\n  voicePath: apiResponse.voice_path || null\n};"
            },
